{
    "version" : "1.2.0",
    "authors" : ["jason(jasonsoop@gmail.com)"],
    "description" : "This is a kernel library about Tiandao development framework.",
    "frameworks" : {
        "net45": {
            "frameworkAssemblies": {
                "System.Xml": "4.0.0.0",
                "System.Xml.ReaderWriter": "4.0.0.0",
                "System.Linq": "4.0.0.0",
                "System.Linq.Expressions": "4.0.0.0",
                "System.Runtime": "4.0.0.0",
                "System.Runtime.Caching": "4.0.0.0"
            }
        },

        "net46": {
            "frameworkAssemblies": {
                "System.Xml": "4.0.0.0",
                "System.Xml.ReaderWriter": "4.0.10.0",
                "System.Linq": "4.0.0.0",
                "System.Linq.Expressions": "4.0.10.0",
                "System.Runtime": "4.0.20.0",
                "System.Runtime.Caching": "4.0.0.0"
            },
            "define": [ "PLAT_SAFE_CONTINUATIONS" ]
        },

        "netstandard1.6": {
            "imports": [ "dotnet5.6", "dnxcore50", "portable-net45+win8" ],
            "buildOptions": {
                "define": [ "PLAT_SAFE_CONTINUATIONS", "CORE_CLR" ]
            },
            "dependencies": {
                "NETStandard.Library": "1.6.0",
                "System.Runtime.Loader": "4.0.0",
                "System.Data.Common": "4.1.0",
                "System.IO.FileSystem.DriveInfo": "4.0.0",
                "System.Threading.ThreadPool": "4.0.10",
                "System.Threading.Tasks.Parallel": "4.0.1",
                "System.Reflection.TypeExtensions": "4.1.0",
                "System.ComponentModel.TypeConverter": "4.1.0",
                "System.Security.Principal": "4.0.1",
                "System.Security.Cryptography.Algorithms": "4.2.0",
                "System.Runtime.Serialization.Primitives": "4.1.1",
                "Microsoft.Extensions.Caching.Memory": "1.0.0",
                "Microsoft.Extensions.Primitives": "1.0.0",
                "Microsoft.Extensions.PlatformAbstractions": "1.0.0",
                "Microsoft.Extensions.DependencyModel": "1.0.0"
            }
        }
    },
    "buildOptions" : {
        "xmlDoc" : true
    }
}
